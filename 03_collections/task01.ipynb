    "print(f\"平均点: {average}\")\n",
    "print(f\"最高点: {max(grades)}\")\n",
    "print(f\"最低点: {min(grades)}\")\n",
    "print(f\"平均以上の学生数: {len(list(filter(lambda x: True if x >= average else False, grades)))}\")"
